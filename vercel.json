{
  "version": 2,
  "framework": "nextjs",
  "functions": {
    "app/api/proxy/[...path]/route.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*",
      "permanent": false
    }
  ]
} 